<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.batchmates.android.scramblepuzzle">

    <application
        android:allowBackup="true"
        android:icon="@drawable/puzzle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/SplashScreen">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ThreeByThree" android:screenOrientation="portrait"/>
        <activity android:name=".FourByFour" android:screenOrientation="portrait"/>
        <activity android:name=".FiveByFive" android:screenOrientation="portrait"></activity>
    </application>

</manifest>